<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cashierapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CashierNative">
        <activity
            android:name="com.cashierapp.Activities.Cashier.CashierActivity"
            android:label="@string/title_activity_cashiers"
            android:theme="@style/Theme.CashierNative.NoActionBar" />
        <activity
            android:name="com.cashierapp.Activities.Cashier.CashiersActivity"
            android:label="@string/title_activity_cashiers"
            android:theme="@style/Theme.CashierNative.NoActionBar"/>
        <activity
            android:name="com.cashierapp.Activities.AboutActivity"
            android:label="@string/title_activity_cashiers"
            android:theme="@style/Theme.CashierNative.NoActionBar"/>
        <activity
            android:name="com.cashierapp.Activities.History.HistoryActivity"
            android:label="@string/title_activity_cashiers"
            android:theme="@style/Theme.CashierNative.NoActionBar"/>
        <activity
            android:name="com.cashierapp.Activities.History.HistoryDateActivity"
            android:label="@string/title_activity_cashiers"
            android:theme="@style/Theme.CashierNative.NoActionBar"/>
        <activity
            android:name="com.cashierapp.Activities.History.HistoryDetailActivity"
            android:label="@string/title_activity_cashiers"
            android:theme="@style/Theme.CashierNative.NoActionBar"/>
        <activity
            android:name="com.cashierapp.Activities.MainActivity"
            android:theme="@style/Theme.CashierNative.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
